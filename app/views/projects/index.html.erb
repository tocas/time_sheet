<h1>Listing projects</h1>

<table class="table table-striped">
  <tr>
    <th>Project name</th>
    <th>Actions</th>
  </tr>
  

<% @projects.each do |project| %>

<tr>
  <td><%= link_to project.name, project_path(project.id) %></td>
  <td><%= link_to "Overview", project_overview_path(project.id), :class => "btn btn-primary btn-mini"%></td>
</tr>


<% end %>
</table>


