<h1>Listing projects</h1>

<p><%= link_to 'New Project', new_project_path, :class => 'btn btn-large btn-primary'%></p>

<table class="table table-striped">
  <tr>
    <th>Name</th>
  </tr>

<% @projects.each do |project| %>
  <tr>
    <td><%= link_to project.name, project_path(project.id) %></td>
  </tr>
<% end %>
</table>

<br />

